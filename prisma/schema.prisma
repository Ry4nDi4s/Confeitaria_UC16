// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Product {
  id Int
  
  description String
  name String
  quantify Int
  stock Float
  maturity Date

  createdAt DateTime
  updatedAt DateTime
}
