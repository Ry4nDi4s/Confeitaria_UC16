// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
model Order {
  id Int

  order String
  who_request String
  what_value String
  quantity_String

  createAt DateTime
  updateAt DataTime


}

