// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model payment {
  id Int

  card String
  pix String
  money Float
  value Float
  scheduling String
  createdAt DateTime 
  updatedAt DateTime 
  
}